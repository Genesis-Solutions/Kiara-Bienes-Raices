<div class="px-5 py-3 text-center">
    <div class="grid grid-cols-2 gap-2 py-2 text-sm">
        <div>
            <% if (inmueble.m2ConstruidosInmueble == 0) { %>
                <i class="fa-solid fa-house invisible"></i>
            <% } else if (inmueble.m2ConstruidosInmueble > 0){ %>
            <i class="fa-solid fa-house"></i> <%= inmueble.m2ConstruidosInmueble %> m2 Construidos
            <% } %>
        </div>
        <div>
            <% if (inmueble.nivelesInmueble == 0) { %>
                <i class="fa-solid fa-layer-group invisible"></i>
            <% } else if (inmueble.nivelesInmueble > 0){ %>
            <i class="fa-solid fa-layer-group"></i> <%= inmueble.nivelesInmueble %> piso(s)
            <% } %>
        </div>
        <div>
            <% if (inmueble.baniosInmueble == 0) { %>
                <i class="fa-solid fa-bath invisible"></i>
            <% } else if (inmueble.baniosInmueble > 0) { %>
            <i class="fa-solid fa-bath"></i> <%= inmueble.baniosInmueble %> ba√±o(s)
            <% } %>
        </div>
        <div>
            <% if (inmueble.estacionamientosInmueble == 0) { %>
                <i class="fa-solid fa-car invisible"></i>
            <% } else if (inmueble.estacionamientosInmueble > 0) { %>
            <i class="fa-solid fa-car"></i> <%= inmueble.estacionamientosInmueble %> estac.
            <% } %>
        </div>
    </div>
</div>